#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    ll day,month,year,all_count=0,leap_year;
    cout<<"HELLO !"<<endl<<endl<<"HOW ARE YOU TODAY ?"<<endl<<endl;
    cout<<"PLEASE, ENTER THE DAY "<<endl;
    cin>>day;
    cout<<"PLEASE, ENTER THE MONTH "<<endl;
    cin>>month;
    cout<<"PLEASE, ENTER THE YEAR "<<endl;
    cin>>year;
    cout<<endl;
    
    if(year%400==0 or (year%4==0 and year%100!=0))leap_year=1;
    else leap_year=0;
    
    if(leap_year and month==1)all_count=-1;
    else if(leap_year and month==2)all_count=2;
    else if(month==2 or month==3 or month==11)all_count+=3;
    else if(month==4 or month==7)all_count+=6;
    else if(month==5)all_count+=1;
    else if(month==6)all_count+=4;
    else if(month==8)all_count+=2;
    else if(month==9 or month==12)all_count+=5;
    
    all_count+=(day+(year-1900)+(year-1900)/4);
    
    if(all_count%7==0)cout<<"THAT DAY WAS SUNDAY"<<endl;
    else if(all_count%7==1)cout<<"THAT DAY WAS MONDAY"<<endl;
    else if(all_count%7==2)cout<<"THAT DAY WAS TUESDAY"<<endl;
    else if(all_count%7==3)cout<<"THAT DAY WAS WEDNESDAY"<<endl;
    else if(all_count%7==4)cout<<"THAT DAY WAS THURSDAY"<<endl;
    else if(all_count%7==5)cout<<"THAT DAY WAS FRIDAY"<<endl;
    else cout<<"THAT DAY WAS SATURDAY"<<endl;
    cout<<endl;
    cout<<"THANK YOU !"<<endl<<endl<<"PLEASE, HAVE A CHECK WHETHER IT IS WRONG OR RIGHT WITH ANOTHER CALENDER";
    return 0;
}
